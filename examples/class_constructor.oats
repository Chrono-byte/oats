export class Counter {
    private value: number;

    constructor(initialValue: number) {
        this.value = initialValue;
    }

    increment(): number {
        this.value += 1;
        return this.value;
    }

    getValue(): number {
        return this.value;
    }
}

export function main(): void {
    let counter = new Counter(10);
    counter.increment();
    println(counter.getValue());
}