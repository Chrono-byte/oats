export class Counter {
    constructor(public value: number) {
        // Constructor parameter 'value' automatically becomes a field
    }

    increment(): number {
        this.value = this.value + 1.0;
        return this.value;
    }

    getValue(): number {
        return this.value;
    }
}

export function main(): number {
    let c = new Counter(5.0);
    print_f64(c.getValue());
    c.increment();
    print_f64(c.getValue());
    return 0;
}
