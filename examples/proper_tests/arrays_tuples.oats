export function main(): void {
    // An array of numbers
    let fibonacci: number[] = [1, 1, 2, 3, 5, 8];

    // A tuple with a specific set of types
    let person: [string, number] = ["Alice", 30];

    console.log("Fibonacci sequence: ", fibonacci);
    console.log(`Person: ${person[0]}, Age: ${person[1]}`);

    return;
}
