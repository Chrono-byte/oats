export function main(): void {
    // All variables with explicit types
    let framework: string = "boop";
    let version: number = 3.2;
    let isAwesome: boolean = true;

    let explicitString: string = "This should be a string.";
    let explicitNumber: number = 42;

    println(`Framework: ${framework}`);
    println(`Version: ${version}`);
    println(`Is it awesome? ${isAwesome}`);
    println(`Explicit String: ${explicitString}`);
    println(`Explicit Number: ${explicitNumber}`);

    return;
}
