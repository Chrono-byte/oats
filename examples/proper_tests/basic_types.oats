export function main(): void {
    // Explicit typing
    let framework: string = "TypeScript";
    let version: number = 5.2;
    let isAwesome: boolean = true;

    // Type inference
    let inferredString = "This should be a string.";
    let inferredNumber = 42;

    println(`Framework: ${framework}`);
    println(`Version: ${version}`);
    println(`Is it awesome? ${isAwesome}`);
    println(`Inferred String: ${inferredString}`);
    println(`Inferred Number: ${inferredNumber}`);

    return;
}
