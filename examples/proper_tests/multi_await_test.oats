export function main(): void {
    // Helper function that returns a resolved promise
    function getMessage(msg: string): Promise<string> {
        return Promise.resolve(msg);
    }

    async function testMultipleAwaits(): Promise<void> {
        console.log("Starting multi-await test...");
        
        let first: string = await getMessage("First message");
        console.log(first);
        
        let second: string = await getMessage("Second message");
        console.log(second);
        
        let third: string = await getMessage("Third message");
        console.log(third);
        
        console.log("All awaits completed!");
    }

    // Run the async function
    let _p: Promise<void> = testMultipleAwaits();

    return;
}
