export function main(): void {
    const name = "World";
    const count = 42;
    const flag = true;
    
    // Simple interpolation
    const greeting = `Hello, ${name}!`;
    println(greeting);
    
    // Multiple expressions
    const message = `Count is ${count} and flag is ${flag}`;
    println(message);
    
    // With calculations
    const calc = `Result: ${count * 2}`;
    println(calc);
}
