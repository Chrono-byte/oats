// Test object as function parameter with interface/type

export function main(): number {
    // Define a point
    let point: { x: number, y: number } = { x: 100, y: 200 };
    
    // Pass to function with explicit nominal type
    function getX(p: { x: number, y: number }): number {
        return p.x;
    }
    
    let result: number = getX(point);
    console.log("Result:");
    console.log(result);
    
    return 0;
}
