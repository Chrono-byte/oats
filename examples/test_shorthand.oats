// Test shorthand properties separately

export function main(): number {
    let x: number = 5;
    let y: number = 15;
    let coords: { x: number, y: number } = { x, y };
    let sum2: number = coords.x + coords.y;
    
    println("Shorthand test:");
    println(coords.x);
    println(coords.y);
    println(sum2);
    
    return 0;
}
