// Test program to demonstrate the Oats standard library
// This would be compiled with oatsc and linked with oats_std

export function main(): number {
    // Test console functions
    console.log("Hello from Oats standard library!");
    console.log_number(42.5);
    console.log_int(123);
    console.log_newline();

    // Test fs functions
    let exists = fs.file_exists("Cargo.toml");
    console.log_int(exists);

    // Test env functions
    let exe_path = env.current_exe();
    if (exe_path) {
        console.log(exe_path);
    }

    // Test time functions
    let now = time.now();
    console.log_number(now);

    return 0;
}