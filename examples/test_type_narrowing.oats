// Test type narrowing with typeof guards
export function main(): void {
    let x: number | string = 42;
    
    if (typeof x === "number") {
        console.log(x);  // x is number here, should print: 42
    }
    
    x = "hello";
    
    if (typeof x === "string") {
        console.log(x);  // x is string here, should print: hello
    }
    
    // Test with else branch
    let y: number | string = 100;
    
    if (typeof y === "string") {
        console.log(y);  // This won't execute
    } else {
        console.log(y);  // y should still be union here, should print: 100
    }
}
