// Simple union test to debug issues

export function main(): void {
    console.log("Test 1: Basic number");
    let x: number = 42;
    console.log(x);
    
    console.log("Test 2: Basic string");
    let s: string = "hello";
    console.log(s);
    
    console.log("Test 3: Number union assignment");
    let u: number | string = 100;
    console.log(u);
    
    console.log("Test 4: String union assignment");
    let v: number | string = "world";
    console.log(v);
    
    console.log("Test 5: Typeof on number");
    let n: number | string = 5;
    if (typeof n === "number") {
        console.log("is number");
        console.log(n);
    } else {
        console.log("not number");
    }
    
    console.log("Test 6: Typeof on string");
    let t: number | string = "test";
    if (typeof t === "string") {
        console.log("is string");
        console.log(t);
    } else {
        console.log("not string");
    }
    
    console.log("Done");
}
